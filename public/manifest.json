{
  "manifest_version": 2,
  "name": "React Content Script",
  "version": "0.1",
  "description": "React Content Script",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"]
    }
  ]
}
